{
  "name": "promise-que",
  "version": "0.0.0",
  "description": "Promise based queue with rich API.",
  "main": "lib/index.js",
  "scripts": {
    "test": "NODE_ENV=test _mocha -u tdd -r babel-register --timeout 10000 ./test/bootstrap.js ./test/*.test.js",
    "coverage": "NODE_ENV=test istanbul cover _mocha -- -u tdd -r babel-register --timeout 10000 ./test/bootstrap.js ./test/*.test.js",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "build": "babel src -d lib",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lividum/promise-que.git"
  },
  "keywords": [
    "promise",
    "queue",
    "throttle"
  ],
  "author": "Andi N. Dirgantara",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lividum/promise-que/issues"
  },
  "homepage": "https://github.com/lividum/promise-que#readme",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.6.0",
    "bluebird": "^3.4.1",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.13.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "istanbul": "1.0.0-alpha.2",
    "jsdoc": "^3.4.0",
    "mocha": "^2.4.5"
  }
}
